{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@": "<rootDir>/",
    "@controller": "<rootDir>/controller",
    "@service": "<rootDir>/core/service",
    "@core": "<rootDir>/core"
  }
}
